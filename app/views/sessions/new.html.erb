<h1>login</h1>

<%= form_with(url: sessions_path, scope: :session) do |f| %>

    <%= f.label :email, 'email' %>
    <%= f.email_field :email %>
    
    <%= f.label :password, 'Password' %>
    <%= f.password_field :password %>
    
    <%= f.submit 'Log In' %>
    
    <%= link_to 'Sign up', new_user_path %>
    
<% end %>